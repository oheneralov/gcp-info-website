header.section.page-header
    // RD Navbar
    .rd-navbar-wrap.rd-navbar-centered
        nav.rd-navbar(data-layout='rd-navbar-fixed'
            data-sm-layout='rd-navbar-fixed'
            data-md-layout='rd-navbar-fixed'
            data-lg-layout='rd-navbar-fullwidth'
            data-xl-layout='rd-navbar-static'
            data-xxl-layout='rd-navbar-static'
            data-xxxl-layout='rd-navbar-static'

            data-md-device-layout='rd-navbar-fixed'
            data-lg-device-layout="rd-navbar-static"

            data-md-stick-up-offset="1px"
            data-lg-stick-up-offset="1px"
            data-xl-stick-up-offset="1px"
            data-xxl-stick-up-offset="1px"
            data-xxxl-stick-up-offset="1px"

            data-stick-up='true'
            data-sm-stick-up='true'
            data-md-stick-up='true'
            data-lg-stick-up='true'
            data-xl-stick-up='true'
            data-xxl-stick-up='true'
            data-xxxl-stick-up='true'

        )
            .rd-navbar-inner

                // RD Navbar Panel
                .rd-navbar-panel
                    // RD Navbar Toggle
                    button.rd-navbar-toggle(data-rd-navbar-toggle='.rd-navbar-nav-wrap'): span

                    // RD Navbar Brand
                    .rd-navbar-brand
                        a.brand-name(href='index.html')
                            +png(logoDefault, 'logo-default')

                .rd-navbar-aside-left
                    .rd-navbar-nav-wrap
                        +navigation(menu)